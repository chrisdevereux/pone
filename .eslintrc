{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ],
  "env": {
    "node": true,
    "mocha": true,
    "browser": true
  },
  "rules": {
    "strict": [1, "never"],
    "quotes": [1, "single"],
    "no-underscore-dangle": [0],
    "no-use-before-define": [0],
    "no-unused-expressions": [0],
    "new-cap": [0]
  }
}
